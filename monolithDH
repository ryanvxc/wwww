    Main = {
        Type = "FOV",
        TargetAim = false,
        Part = "Head",
        Keybinds = "C",
        Predictions = 0,
        AutoPrediction = false,
        Resolver = false,
        View = false,
        Aimbot = false,
        AimbotPred = 0.12,
        AimbotParts = "HumanoidRootPart",
        Smoothness = 1,
        AimbotKeybind = "C",
        LookAt = false,
    },

    Visuals = {
        Circle = false;
        CircleColor = Color3.fromRGB(255, 255, 255);
